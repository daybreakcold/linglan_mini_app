/* 健康档案表单 */
.health-form {
  padding: 40rpx;
  background-color: #ffffff;
  border-radius: 30rpx;
  box-shadow: 0rpx 2rpx 12rpx #2b78ed08;
}
.form-title {
  font-size: 32rpx;
  color: #1c2b42;
  font-weight: 600;
}
.form-item {
  margin-top: 28rpx;
  display: flex;
  flex-direction: column;
}
.label {
  font-size: 26rpx;
  color: #1c2b42;
  margin-bottom: 12rpx;
}
.input {
  padding: 20rpx;
  border: 1rpx solid #e5e5e5;
  border-radius: 16rpx;
  background: #f9f9f9;
}
.textarea {
  padding: 20rpx;
  border: 1rpx solid #e5e5e5;
  border-radius: 16rpx;
  background: #f9f9f9;
  min-height: 160rpx;
}
.gender-group {
  display: flex;
  flex-direction: row;
  gap: 32rpx;
}
.gender-item {
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #1c2b42;
}
.form-actions {
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  gap: 20rpx;
}
.btn {
  flex: 1;
  padding: 20rpx 0;
  border-radius: 16rpx;
  font-size: 28rpx;
}
.btn.cancel {
  background: #f2f2f2;
  color: #1c2b42;
  border: 1rpx solid #e5e5e5;
}
.btn.primary {
  background: linear-gradient(283.1deg, #217aff 1.8%, #5c87ff 97.4%);
  color: #ffffff;
  border: none;
}
